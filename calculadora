numero1 = int(input("Digite o primeiro número inteiro: "))
numero2 = int(input("Digite o segundo número inteiro: "))

soma = numero1 + numero2
subtracao = numero1 - numero2
multiplicacao = numero1 * numero2
divisao = numero1 / numero2  

print(f"A soma de", numero1, "por", numero2, "é:", soma)
print(f"A subtração de", numero1, "por", numero2, "é:", subtracao)
print(f"A multiplicação de", numero1, "por", numero2, "é:", multiplicacao)
print("A divisão de", numero1,"por", numero2, "é:", divisao)
